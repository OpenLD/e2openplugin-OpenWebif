#filter WebSafe
/* 
	All Themes:
	base
	black-tie
	blitzer
	cupertino
	dark-hive
	dot-luv
	eggplant
	excite-bike
	flick
	hot-sneaks
	humanity
	le-frog
	mint-choc
	overcast
	pepper-grinder
	redmond
	smoothness
	south-street
	start
	sunny
	swanky-purse
	trontastic
	ui-darkness
	ui-lightness
	vader
*/

#set $t="redmond"
#if $theme
	#set $t=$theme
#end if

#if $theme == "excite-bike"
#header { background : #eeeeee;}
#end if

#if $theme == "pepper-grinder"
#header { background : #eceadf;}
.off { color : #67A0F5;}
#end if

#if $theme == "vader"
.off { color : #000000;}
#end if

#if $theme == "sunny"
.off { color : #feeebd;}
#end if

#if $theme == "south-street"
.off { color : #327E04;}
#end if

#if $theme == "smoothness"
.off { color : #e6e6e6;}
#end if

#if $theme == "redmond"
.off { color : #e17009;}
#end if

#if $theme == "mint-choc"
.off { color : #9bcc60;}
#end if

#if $theme == "le-frog"
.off { color : #4ca20b;}
#end if

#if $theme == "humanity"
.off { color : #ede4d4;}
#end if

#if $theme == "hot-sneaks"
.off { color : #93c3cd;}
#end if

#if $theme == "eggplant"
.off { color : #7e7783;}
#end if

#if $theme == "dark-hive"
.off { color : #000000;}
#end if

#if $theme == "blitzer"
.off { color : #d8dcdf;}
#end if

#if $theme == "base"
#header { background : #007fff;}
.off { color : #c5c5c5;}
#end if

#end filter